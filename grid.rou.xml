    <flow id="flow_bus_2" type="bus" begin="0" end="36000" from="n10_n11" to="n11_n12" vehsPerHour="500" departPos="random" departLane="0"/>
    <flow id="flow_bus_3" type="bus" begin="0" end="36000" from="n21_n11" to="n11_n01" vehsPerHour="500" departPos="random" departLane="0"/>
