    <flow id="flow_cars_n22" type="car" begin="0" end="36000" from="n20_n21" to="n21_n22" vehsPerHour="150" departPos="random" departLane="best"/>

    <!-- Fluxos de carros passando pelo RL n11 (alta densidade) -->
