<routes>
    <!-- Tipos de veículos -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" maxSpeed="13.9"/>
    <vType id="bus" accel="1.5" decel="3.0" sigma="0.5" length="12" maxSpeed="12.0"/>

    <!-- Fluxos de carros nos fixos/adaptativos (menor densidade) -->
    <flow id="flow_cars_n00" type="car" begin="0" end="36000" from="n00_n01" to="n01_n02" vehsPerHour="200" departPos="random" departLane="best"/>
    <flow id="flow_cars_n02" type="car" begin="0" end="36000" from="n02_n12" to="n12_n22" vehsPerHour="150" departPos="random" departLane="best"/>
    <flow id="flow_cars_n20" type="car" begin="0" end="36000" from="n20_n10" to="n10_n00" vehsPerHour="200" departPos="random" departLane="best"/>
    <flow id="flow_cars_n12" type="car" begin="0" end="36000" from="n12_n11" to="n11_n10" vehsPerHour="200" departPos="random" departLane="best"/>
    <flow id="flow_cars_n01" type="car" begin="0" end="36000" from="n01_n11" to="n11_n21" vehsPerHour="200" departPos="random" departLane="best"/>
